{% extends 'base.html.twig' %}

{% block title %}Créer un utilisateur - {{ parent() }}{% endblock %}

{% block body %}

    <h1>Créer un nouvel Utilisateur</h1>
    {{ form_start(form) }}
    {{ form_row(form.isAdmin) }}
    {{ form_widget(form) }}
    <button>Valider</button>
    {{ form_end(form) }}


    <a href="{{ path('user_index') }}">Retour à la Liste</a>


{% endblock %}
